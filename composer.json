{
  "name": "rawkode/eidetic",
  "description": "CQRS and EventSourcing package for php >= 5.4",
  "license": "MIT",
  "type": "library",
  "authors": [
      {
          "name": "David McKay",
          "email": "david@rawkode.com"
      }
  ],
  "keywords": [
    "CQRS",
    "EventSourcing",
    "EventStore"
  ],
  "autoload": {
    "psr-4": {
      "Rawkode\\Eidetic\\": "src//"
    }
  },
  "autoload-dev": {
      "psr-4": {
          "Example\\": "usr//share//doc//example//"
      }
  },
  "config": {
    "bin-dir": "bin",
    "vendor-dir": "var/vendor"
  },
  "require": {
    "php": ">=5.5",
    "doctrine/dbal": "^2.5",
    "symfony/event-dispatcher": "^2.7"
  },
  "require-dev": {
    "phpspec/phpspec": "^2.1",
    "phpunit/phpunit": "^4.3",
    "fabpot/php-cs-fixer": "^1.10"
  }
}
